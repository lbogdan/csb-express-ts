{
  "name": "csb-express-ts",
  "description": "CodeSandbox Express TypeScript Template",
  "keywords": [
    "codesandbox",
    "express",
    "typescript"
  ],
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "rm -fr dist",
    "dev": "tsc-watch --noClear --onSuccess \"node dist/index.js\"",
    "start": "node dist/index.js",
    "watch": "tsc --watch --preserveWatchOutput"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.7",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.4.4"
  }
}
